{
  "name": "react-ecommerce-main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:users\" \"npm run server:products\" \"npm run server:orders\" \"vite\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:users": "json-server --watch backend/users.json --port 3001",
    "server:products": "json-server --watch backend/products.json --port 3000",
    "server:orders": "json-server --watch backend/orders.json --port 3002",
    "updater": "node src/order-management/statusUpdater.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/vite": "^4.1.13",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "json-server": "^1.0.0-beta.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.1",
    "react-router-hash-link": "^2.4.3",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.5"
  }
}
